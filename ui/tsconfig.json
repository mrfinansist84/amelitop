{
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": ".",
    "jsx": "react",
    "module": "es6",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noUnusedLocals": false,
    "outDir": "./dist/",
    "paths": {
      "~*": ["src/*"]
    },
    "sourceMap": true,
    "target": "es5"
  }
}
